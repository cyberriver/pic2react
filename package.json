{
  "name": "react-component-generator",
  "version": "0.1.0",
  "description": "Автоматическая генерация React компонентов из изображений UI элементов",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "build": "npm run build:web-ui",
    "build:web-ui": "cd web-ui && npm run build",
    "install:web-ui": "cd web-ui && npm install",
    "test": "jest",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "keywords": [
    "react",
    "component",
    "generator",
    "computer-vision",
    "ui",
    "automation"
  ],
  "author": "CyberRiver",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.4",
    "chokidar": "^3.5.3",
    "handlebars": "^4.7.8",
    "openai": "^4.20.1",
    "faker": "^6.6.6",
    "zod": "^3.22.4",
    "prettier": "^3.1.0",
    "eslint": "^8.55.0",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "glob": "^10.3.10"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "@types/node": "^20.10.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
